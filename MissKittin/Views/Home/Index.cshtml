@model List<MissKittin.Models.Cat>

<div class="jumbotron">
    <h1>Miss Kittin</h1>
    <p class="lead">Vote up your favourite cat</p>
</div>
<div class="row">
@{
    var step = 3;
    for (var i = 0; i < Model.Count / 3; i++)
    {
        for (var j = 0; j < 3; j++)
        {
            <div class="col-md-4">
                <img src=@Model[i * 3 + j].Url class="img-rounded" width="300" height="200"/>
            </div>
        }
    }
}
</div>
